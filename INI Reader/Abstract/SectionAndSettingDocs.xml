<?xml version="1.0" encoding="utf-8" ?>

<docs>

  <section>
    <summary>
      An extensible <see langword="Enum"/> used to provide intellisense for section names.
    </summary>
    <remarks>
      <para>Example:</para>
      <code>
        <see langword="Public"/> <see langword="Class"/> Section<br/>
        <see langword="Inherits"/> <see cref="UserDefinedSection"/><br/>
        <see langword="Protected"/> <see langword="Sub"/> <see langword="New"/>(<see langword="ByVal"/> Value <see langword="As"/> <see langword="Integer"/>, <see langword="ByVal"/> DisplayName <see langword="As"/> <see langword="String"/>)<br/>
        <see langword="MyBase"/>.<see langword="New"/>(Value, DisplayName)<br/>
        <see langword="End"/> <see langword="Sub"/><br/>
        <see langword="Public"/> <see langword="Shared"/> <see langword="ReadOnly"/> General <see langword="As"/> Section = <see langword="New"/> Section(0, "General")<br/>
        <see langword="Public"/> <see langword="Shared"/> <see langword="ReadOnly"/> Favorites <see langword="As"/> Section = <see langword="New"/> Section(1, "Favorites")<br/>
        <see langword="End"/> <see langword="Class"/>
      </code>
    </remarks>    
  </section>

  <setting>
     <summary>
           An extensible <see langword="Enum"/> used to provide intellisense for setting names.
    </summary>
     <remarks>
       <para>Example:</para>
        <see langword="Public"/> <see langword="Class"/> Setting<br/>
        <see langword="Inherits"/> <see cref="UserDefinedSetting"/><br/>
        <see langword="Protected"/> <see langword="Sub"/> <see langword="New"/>(<see langword="ByVal"/> Value <see langword="As"/> <see langword="Integer"/>, <see langword="ByVal"/> DisplayName <see langword="As"/> <see langword="String"/>)<br/>
        <see langword="MyBase"/>.<see langword="New"/>(Value, DisplayName)<br/>
        <see langword="End"/> <see langword="Sub"/><br/>
        <see langword="Public"/> <see langword="Shared"/> <see langword="ReadOnly"/> TestSetting <see langword="As"/> Setting = <see langword="New"/> Setting(0, "Test Setting")<br/>
        <see langword="Public"/> <see langword="Shared"/> <see langword="ReadOnly"/> AnotherSetting <see langword="As"/> Setting = <see langword="New"/> Setting(1, "Another Setting")<br/>
        <see langword="End"/> <see langword="Class"/><br/>
    </remarks>
  </setting>
</docs>